array = Matrix(4, 1, sfix)

for i in range(2):
    array[i][0] = sfix.get_input_from(0)

for i in range(2):
    array[i + 2][0] = sfix.get_input_from(1)

a = Matrix(2, 1, sfix)
a[0][0] = sfix(1) 
a[1][0] = sfix(2)


b = Matrix(2, 1, sfix)
b[0][0] = sfix(3) 
b[1][0] = sfix(4)

r = (array[0][0] > 0).if_else(a, b)

print_ln("%s", r[0][0].reveal())